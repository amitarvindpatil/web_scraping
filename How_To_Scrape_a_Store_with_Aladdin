    'subcategory_xpath': None,
    'url_xpath': 'div[3]/a/text()[normalize-space() and not(re:test(., '
                 "'(^|\\W)(website|shipping|contact.?us|more.?info|find in "
                 "store)(\\W|$)', "
                 "'i'))]/ancestor-or-self::a[@href][1]/@href[normalize-space()]"}
